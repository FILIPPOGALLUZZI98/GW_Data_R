# 1002872= numero di righe

library(terra)
setwd("D:Personale/Studio/Magistrale/Tesi/zGW_Data/Africa_ASCII/DepthToGroundwater_V2")

dati <- read.table("xyzASCII_dtwmap_v2.txt", header = TRUE)
raster_dati <- rast(matrix(dati$DTWAFRICA_, nrow = 1002872, byrow = TRUE))
ext(raster_dati) <- c(min(dati$X), max(dati$X), min(dati$Y), max(dati$Y))
plot(raster_dati)
